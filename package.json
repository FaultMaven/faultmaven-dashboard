{
  "name": "faultmaven-copilot",
  "description": "AI-powered troubleshooting assistant embedded in your browser (WXT Version)",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
