    "generate-icons": "node scripts/generate-icons.mjs"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@wxt-dev/module-react": "^1.1.3",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "wxt": "^0.20.6",
    "sharp": "^0.33.2"
  }
}
